  <DataSets>
    <DataSet Name="Licenses">
      <Query>
        <DataSourceName>LicenseDataBase</DataSourceName>
        <CommandText>SELECT PoolID, Name ,  Licenses,
Installed, Licenses - ISNULL(Installed, 0) AS Totat_Licenses   FROM LicenseComplianceReport
Order by Totat_Licenses DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PoolID">
          <DataField>PoolID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Installed">
          <DataField>Installed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Licenses">
          <DataField>Licenses</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Totat_Licenses">
          <DataField>Totat_Licenses</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="UsedSoftware">
      <Query>
        <DataSourceName>LicenseDataBase</DataSourceName>
        <CommandText>SELECT DISTINCT LicenseComplianceReport.PoolID , Computers.ResourceID, LicenseComplianceReport.Name, LicenseComplianceReport.Installed,
LicenseComplianceReport.Licenses, LicenseComplianceReport.Used_3_months, LicenseComplianceReport.Used_6_months, LicenseComplianceReport.Used_12_months,  InstalledAndUsedSoftware.Used90, InstalledAndUsedSoftware.Used180, InstalledAndUsedSoftware.Used365 
From LicenseComplianceReport
Inner Join InstalledAndUsedSoftware On LicenseComplianceReport.PoolID = InstalledAndUsedSoftware.PoolID
Inner Join Computers on InstalledAndUsedSoftware.ResourceID = Computers.ResourceID
Order by LicenseComplianceReport.PoolID</CommandText>
      </Query>
